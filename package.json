{
	"name": "atomico-game-of-life",
	"description": "Conway's Game of Life with Atomico",
	"version": "1.0.0",
	"source": "index.html",
	"scripts": {
		"build": "bundle index.html dist",
		"build:web-components": "bundle src/components/**/*-*.js web-components --external",
		"dev": "bundle index.html dist --watch --server",
		"prepare": "npm run build"
	},
	"dependencies": {
		"atomico": "^0.21.1",
		"rxjs": "^6.5.4"
	},
	"devDependencies": {
		"bundle-cli": "^0.21.5"
	}
}
