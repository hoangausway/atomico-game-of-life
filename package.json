{
	"name": "atomico-game-of-life",
	"description": "Conway's Game of Life with Atomico",
	"version": "1.0.0",
	"source": "index.html",
	"scripts": {
		"build": "bundle index.html dist",
		"build:web-components": "bundle src/web-components/**/*-*.js web-components --external",
		"dev": "bundle index.html dist --server 3000",
		"prepare": "npm run build"
	},
	"dependencies": {
		"atomico": ">=0.8",
		"rxjs": "^6.5.3"
	},
	"devDependencies": {
		"bundle-cli": "^0.6.3"
	}
}
